@model Project.Skillz.Models.Interview
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>New</title>
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/css")

    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/jquery-3.4.1.js"></script>
</head>
<body class="container">
    <form>
        <div class="form-group">
            <label for="usr" style="text-align: left; font: normal normal normal 14px/12px Lucida Console; letter-spacing: 0px; color: #73737A; opacity: 1;">Interview Name</label>
            <input type="text" class="form-control" id="usr" style="background: #FFFFFF 0% 0% no-repeat padding-box; border: 1px solid #707070; border-radius: 7px; opacity: 1;" placeholder="Enter interview name.">
        </div>
    <div class="form-group">
        <label for="sel1" style="text-align: left; font: normal normal normal 14px/12px Lucida Console; letter-spacing: 0px; color: #73737A; opacity: 1;">Skill Level</label>
        <select class="form-control" style="background: #FFFFFF 0% 0% no-repeat padding-box; border: 1px solid #707070; border-radius: 7px; opacity: 1;" id="sel1">
            <option>Begineer</option>
            <option>Intermediate</option>
            <option>Advanced</option>
        </select>
    </div>
    <label for="assmode" style="text-align: left; font: normal normal normal 14px/12px Lucida Console; letter-spacing: 0px; color: #73737A; opacity: 1;">Assesment Modes</label>
    <div class="form-group" id="assmode">
        <label class="radio-inline"><input type="radio" name="optradio" checked>Subjective</label>
        <label class="radio-inline"><input type="radio" name="optradio">Objective</label>
        <label class="radio-inline"><input type="radio" name="optradio">Both</label>
    </div>
        </form>
</body>
</html>
