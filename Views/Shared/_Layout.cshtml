<!DOCTYPE html>
<html>
<head>

    <title>@ViewBag.Title - My ASP.NET Application</title>
    @*<link href="~/bootstrap-4.5.3-dist/css/bootstrap.css" rel="stylesheet" />
        <script src="~/bootstrap-4.5.3-dist/js/bootstrap.js"></script>*@
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/css")
    @*<link href="~/Content/sty.css" rel="stylesheet" />*@
    @*<link href="~/Content/bootstrap.css" rel="stylesheet" />*@
    <script src="~/Scripts/bootstrap.js"></script>
    <script src="~/Scripts/jquery-3.4.1.js"></script>
</head>
<body>


    <nav class="navbar navbar-inverse" style="border-radius:0px 0px; background: #B70320 0% 0% no-repeat padding-box; opacity: 0.9;">
        <div class="container-fluid">
            <div class="navbar-header">
                <a class="navbar-brand" style=" text-align: left; font: normal normal normal 14px/12px Lucida Console; letter-spacing: 0px; color: #FFFFFF; opacity: 1;">SKILLZ</a>
            </div>
            <ul class="nav navbar-nav" style=" text-align: left; font: normal normal normal 10px/12px Arial; letter-spacing: 0px; color: #FFFFFF; opacity: 1;">
                <li class="active"><a href="#">DASHBOARD</a></li>
                <li><a href="#">INTERVIEW</a></li>
                <li><a href="#">SKILL GROUPS</a></li>
                <li><a href="#">BLOG</a></li>
                <li><a href="#">CONTACT</a></li>
            </ul>
            <ul class="nav navbar-nav navbar-right" style="text-align: left; font: normal normal normal 10px/12px Arial; letter-spacing: 0px; color: #FFFFFF; opacity: 1;">
                <li><a href="#"><span class="glyphicon glyphicon-search"></span></a></li>
                <li><a href="#"><span class="glyphicon glyphicon-bell"></span></a></li>
                <li><a href="#"><span class="glyphicon glyphicon-user"></span></a></li>
            </ul>
        </div>
    </nav>
    <div class="container-fluid body-content">
        @RenderBody()

    </div>     <hr />
    <footer class="container-fluid">
        <p class="footer">&copy; @DateTime.Now.Year - Yukti Solutions Private Limited. All Rights Reserved.</p>
    </footer>

    
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
